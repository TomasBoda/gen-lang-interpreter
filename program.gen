func fibonacci_recursive(var number) {
    if (number <= 1) {
        return number;
    }

    return fibonacci_recursive(number - 1) + fibonacci_recursive(number - 2);
}

func fibonacci(var n) {
    var i = 0;

    while (i < n) {
        print fibonacci_recursive(i) endl;
        var i = i + 1;
    }
}

func main() {
    var n = 30;
    fibonacci(n);
}